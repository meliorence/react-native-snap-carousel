    "license": "BSD-3-Clause",
    "dependencies": {
        "@types/react-addons-shallow-compare": "^0.14.22",
        "eslint-plugin-react-hooks": "^4.5.0",
        "react-addons-shallow-compare": "^15.6.3"
    },
    "peerDependencies": {
